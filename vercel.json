{
  "rewrites": [
    {
      "source": "/beranda/:path*",
      "destination": "https://shoper-beranda.vercel.app/:path*"
    },
    {
      "source": "/beranda/?ref=install/:path*",
      "destination": "https://shoper-beranda.vercel.app/:path*"
    },
    {
      "source": "/reset-page/:path*",
      "destination": "https://shoper-reset-page.vercel.app/:path*"
    }
  ]

}




